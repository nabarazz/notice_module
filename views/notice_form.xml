<odoo>
	<record id="view_notice_form" model="ir.ui.view">
		<field name="name">
			notice.form
		</field>
		<field name="model">
			website.notice
		</field>
		<field name="arch" type="xml">
			<form string="Notice">
				<sheet>
					<group>
						<field name="title" />
						<field name="subtitle" />
						<field name="attachment_id" widget="many2one_binary" />
						<field name="published" />
					</group>
					<footer>
						<button string="Save" type="object" class="oe_highlight" name="action_save" />
						<button string="Cancel" class="btn-secondary" special="cancel" />
					</footer>
				</sheet>
			</form>
		</field>
	</record>
</odoo>
