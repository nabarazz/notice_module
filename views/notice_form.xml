<!-- views/notice_form.xml -->
<odoo>
	<record id="view_notice_form" model="ir.ui.view">
		<field name="name">
			notice.form
		</field>
		<field name="model">
			website.notice
		</field>
		<field name="arch" type="xml">
			<form string="Notice">
				<sheet>
					<group>
						<field name="title" />
						<field name="subtitle" />
						<field name="attachment_id" />
						<field name="published" />
					</group>
				</sheet>
			</form>
		</field>
	</record>
</odoo>
